    "db:pull": "bun drizzle-kit pull",
    "db:gen": "bun drizzle-kit generate"
  },
  "lint-staged": {
    "*.ts|tsx": "eslint --fix",
    "*.tsx": "cross-env TAILWIND=true pretty-quick --staged",
    "*": "pretty-quick --staged"
  },
  "browserslist": {
    "production": [
      "> 0.1%",
      "not dead",
      "not op_mini all"
    ],
